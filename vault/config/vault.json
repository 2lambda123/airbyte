{
  "listener": {
    "tcp": {
      "address": "0.0.0.0:8400",
      "tls_disable": "true"
    }
  },
  "backend": {
    "file": {
      "path": "/vault/file"
    }
  },
  "ui": "true",
  "default_lease_ttl": "168h",
  "max_lease_ttl": "168h",
  "api_addr": "http://0.0.0.0:8400",
  "policy": {
    "path": {
      "secret/test": {
        "capabilities": ["create", "read", "update", "delete", "list"]
      }
    }
  }
}
