{
  "documentationUrl": "https://docs.airbyte.io/integrations/sources/stripe",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Optimizely Source Spec",
    "type": "object",
    "required": ["client_secret", "client_id"],
    "additionalProperties": false,
    "properties": {
      "client_secret": {
        "type": "string",
        "description": "Optimizely API key (find yours <a href=\"https://world.optimizely.com/documentation/developer-guides/campaign/rest-api/getting-started/#auth\">here</a>).",
        "airbyte_secret": true
      },
      "client_id": {
        "type": "string",
        "description": "Your Optimizely account ID (find yours <a href=\"https://world.optimizely.com/documentation/developer-guides/campaign/rest-api/getting-started/#info\">here</a>)."
      }
    }
  }
}