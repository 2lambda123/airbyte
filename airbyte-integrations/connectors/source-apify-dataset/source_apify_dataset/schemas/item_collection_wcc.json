{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Item collection - Website Content Crawler (WCC)",
  "type": ["null", "object"],
  "additionalProperties": true,
  "properties": {
    "crawl": {
      "properties": {
        "depth": {
          "type": "number"
        },
        "httpStatusCode": {
          "type": "number"
        },
        "loadedTime": {
          "type": "string"
        },
        "loadedUrl": {
          "type": "string"
        },
        "referrerUrl": {
          "type": "string"
        }
      },
      "type": "object"
    },
    "markdown": {
      "type": "string"
    },
    "metadata": {
      "properties": {
        "canonicalUrl": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "languageCode": {
          "type": "string"
        },
        "title": {
          "type": "string"
        }
      },
      "type": "object"
    },
    "text": {
      "type": "string"
    },
    "url": {
      "type": "string"
    }
  }
}
