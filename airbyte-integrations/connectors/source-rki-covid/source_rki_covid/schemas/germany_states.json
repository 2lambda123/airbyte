{
  "$schema": "http://json-schema.org/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer"
    },
    "name": {
      "type": "string"
    },
    "population": {
      "type": "integer"
    },
    "cases": {
      "type": "integer"
    },
    "deaths": {
      "type": "integer"
    },
    "casesPerWeek": {
      "type": "integer"
    },
    "deathsPerWeek": {
      "type": "integer"
    },
    "recovered": {
      "type": "integer"
    },
    "abbreviation": {
      "type": "string"
    },
    "weekIncidence": {
      "type": "number"
    },
    "casesPer100k": {
      "type": "number"
    },
    "delta": {
      "type": "object",
      "properties": {
        "cases": {
          "type": "integer"
        },
        "deaths": {
          "type": "integer"
        },
        "recovered": {
          "type": "integer"
        }
      },
      "required": ["cases", "deaths", "recovered"]
    },
    "hospitalization": {
      "type": "object",
      "properties": {
        "cases7Days": {
          "type": "integer"
        },
        "incidence7Days": {
          "type": "number"
        },
        "date": {
          "type": "string"
        },
        "lastUpdate": {
          "type": "string"
        }
      },
      "required": ["cases7Days", "date", "incidence7Days", "lastUpdate"]
    }
  },
  "required": [
    "abbreviation",
    "cases",
    "casesPer100k",
    "casesPerWeek",
    "deaths",
    "deathsPerWeek",
    "delta",
    "hospitalization",
    "id",
    "name",
    "population",
    "recovered",
    "weekIncidence"
  ]
}
