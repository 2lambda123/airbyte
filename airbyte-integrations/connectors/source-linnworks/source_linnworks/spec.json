    "required": [
      "application_id",
      "application_secret",
      "token"
    ],
    "additionalProperties": false,
    "properties": {
      "application_id": {
        "title": "Application ID",
        "type": "string"
      },
      "application_secret": {
        "title": "Application secret",
        "type": "string",
        "airbyte_secret": true
      },
      "token": {
        "title": "Token",
        "type": "string"
      }
    }
  }
}
