{
  "$schema":"http://json-schema.org/schema#",
  "type":"object",
  "properties":{
    "id":{
      "type":"string"
    },
    "name":{
      "type":"string"
    },
    "enabled":{
      "type":"boolean"
    },
    "currency":{
      "type":"string"
    },
    "creationTime":{
      "type":"string"
    },
    "lastModified":{
      "type":"string"
    },
    "blockedSites":{
      "type":"object",
      "properties":{
        "blockedPublishers":{
          "type":"array",
          "items":{
            "type":"object",
            "properties":{
              "id":{
                "type":"string"
              },
              "name":{
                "type":"string"
              },
              "creationTime":{
                "type":"string"
              },
              "modifiedBy":{
                "type":"string"
              }
            },
            "required":[
              "creationTime",
              "id",
              "name"
            ]
          }
        },
        "blockedSections":{
          "type":"array",
          "items":{
            "type":"object",
            "properties":{
              "id":{
                "type":"string"
              },
              "name":{
                "type":"string"
              },
              "publisher":{
                "type":"object",
                "properties":{
                  "id":{
                    "type":"string"
                  },
                  "name":{
                    "type":"string"
                  }
                },
                "required":[
                  "id",
                  "name"
                ]
              },
              "creationTime":{
                "type":"string"
              },
              "modifiedBy":{
                "type":"string"
              }
            },
            "required":[
              "creationTime",
              "id",
              "name",
              "publisher"
            ]
          }
        }
      }
    },
    "useFirstPartyCookie":{
      "type":"boolean"
    },
    "role":{
      "type":"string"
    },
    "permissions":{
      "type":"array",
      "items":{
        "type":"string"
      }
    }
  },
  "required":[
    "blockedSites",
    "creationTime",
    "currency",
    "enabled",
    "id",
    "lastModified",
    "name",
    "permissions",
    "role",
    "useFirstPartyCookie"
  ]
}
