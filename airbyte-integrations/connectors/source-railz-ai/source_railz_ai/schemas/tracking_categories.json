{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "businessName": {
      "type": "string"
    },
    "serviceName": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "isEditable": {
      "type": "boolean"
    },
    "parentRef": {
      "type": ["null", "object"],
      "properties": {
        "id": {
          "type": "string"
        },
        "name": {
          "type": ["null", "string"]
        }
      }
    },
    "status": {
      "type": "string",
      "enum": [
        "active",
        "archived",
        "unknown"
      ]
    },
    "sourceModifiedDate": {
      "type": ["null", "string"],
      "format": "date-time"
    }
  }
}