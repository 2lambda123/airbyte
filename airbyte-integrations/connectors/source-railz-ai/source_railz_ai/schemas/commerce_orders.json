{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "businessName": {
      "type": "string"
    },
    "serviceName": {
      "type": "string"
    },
    "orderNumber": {
      "type": ["null", "string"]
    },
    "fulfillmentRefs": {
      "type": ["null", "array"],
      "items": {
        "type": ["null", "object"],
        "properties": {
          "carrier": {
            "type": ["null", "string"]
          },
          "trackingNumber": {
            "type": ["null", "string"]
          },
          "service": {
            "type": ["null", "string"]
          },
          "trackingUrl": {
            "type": ["null", "string"]
          }
        }
      }
    },
    "lines": {
      "type": ["null", "array"],
      "items": {
        "type": ["null", "object"],
        "properties": {
          "id": {
            "type": "string"
          },
          "productId": {
            "type": ["null", "string"]
          },
          "variantId": {
            "type": "string"
          },
          "name": {
            "type": ["null", "string"]
          },
          "quantity": {
            "type": "integer"
          },
          "totalPrice": {
            "type": "number"
          },
          "unitPrice": {
            "type": ["null", "number"]
          }
        }
      }
    },
    "refundRefs": {
      "type": ["null", "array"],
      "items": {
        "type": ["null", "object"],
        "properties": {
          "id": {
            "type": "string"
          },
          "totalAmount": {
            "type": "number"
          }
        }
      }
    },
    "transactionSourceRefs": {
      "type": ["null", "array"],
      "items": {
        "type": ["null", "object"],
        "properties": {
          "id": {
            "type": "string"
          },
          "type": {
            "type": "number",
            "enum" : [
              "failedPayout",
              "payment",
              "paymentFee",
              "paymentFeeRefund",
              "payout",
              "refund",
              "transfer",
              "dispute",
              "order",
              "other",
              "unknown"
            ]
          },
          "totalAmount": {
            "type": "number"
          }
        }
      }
    },
    "shippingAddress": {
      "type": ["null", "object"],
      "properties": {
        "contactName": {
          "type": ["string", "null"]
        },
        "phone": {
          "type": ["string", "null"]
        },
        "line1": {
          "type": ["string", "null"]
        },
        "line2": {
          "type": ["string", "null"]
        },
        "city": {
          "type": ["string", "null"]
        },
        "region": {
          "type": ["string", "null"]
        },
        "country": {
          "type": ["string", "null"]
        },
        "postalCode": {
          "type": ["string", "null"]
        }
      }
    },
    "billingAddress": {
      "type": ["null", "object"],
      "properties": {
        "contactName": {
          "type": ["string", "null"]
        },
        "phone": {
          "type": ["string", "null"]
        },
        "line1": {
          "type": ["string", "null"]
        },
        "line2": {
          "type": ["string", "null"]
        },
        "city": {
          "type": ["string", "null"]
        },
        "region": {
          "type": ["string", "null"]
        },
        "country": {
          "type": ["string", "null"]
        },
        "postalCode": {
          "type": ["string", "null"]
        }
      }
    },
    "customerRef": {
      "type": ["null", "object"],
      "properties": {
        "id": {
          "type": "string"
        },
        "customerName": {
          "type": ["null", "string"]
        },
        "email": {
          "type": ["null", "string"]
        }
      }
    },
    "currencyRef": {
      "type": ["null", "object"],
      "properties": {
        "id": {
          "type": ["null", "string"]
        },
        "name": {
          "type": "string"
        },
        "symbol": {
          "type": "string"
        }
      }
    },
    "totalShippingAmount": {
      "type": ["null", "number"]
    },
    "totalGratuityAmount": {
      "type": ["null", "number"]
    },
    "totalDiscountAmount": {
      "type": ["null", "number"]
    },
    "totalTaxAmount": {
      "type": "number"
    },
    "totalAmount": {
      "type": "number"
    },
    "createdDate": {
      "type": "string",
      "format": "date-time"
    },
    "sourceModifiedDate": {
      "type": "string",
      "format": "date-time"
    },
    "status": {
      "type": "string",
      "enum": [
        "open",
        "closed",
        "cancelled",
        "unknown",
        "draft"
      ]
    },
    "paymentStatus": {
      "type": "string",
      "enum": [
        "pending",
        "approved",
        "paid",
        "failed",
        "cancelled",
        "refunded",
        "unknown"
      ]
    },
    "fulfillmentStatus": {
      "type": "string",
      "enum": [
        "fulfilled",
        "partial",
        "unfulfilled",
        "cancelled",
        "unknown"
      ]
    }
  }
}