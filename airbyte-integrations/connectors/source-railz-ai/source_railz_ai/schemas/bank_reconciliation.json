{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "reportId": {
      "type": "string"
    },
    "startDate": {
      "type": "string",
      "format": "date-time"
    },
    "endDate": {
      "type": "string",
      "format": "date-time"
    },
    "businessName": {
      "type": "string"
    },
    "serviceName": {
      "type": "string"
    },
    "bankTransactionsCount": {
      "type": "integer"
    },
    "accountingTransactionsCount": {
      "type": "integer"
    },
    "bankTransactionsAmount": {
      "type": "number"
    },
    "accountingTransactionsAmount": {
      "type": "number"
    },
    "bankTransactionsTotalValue": {
      "type": "number"
    },
    "accountingTransactionsTotalValue": {
      "type": "number"
    },
    "weightedPercentageError": {
      "type": "number"
    },
    "accuracyScore": {
      "type": "number"
    },
    "bankTransactionsUnreconciledCount": {
      "type": "integer"
    },
    "bankTransactionsUnreconciledAmount": {
      "type": "number"
    },
    "dailyAccuracyScore": {
      "type": "number"
    },
    "unreconciledBankTransactions": {
      "type": ["null", "array"],
      "items": {
        "type": ["null", "object"],
        "properties": {
          "id": {
            "type": "string"
          },
          "date": {
            "type": "string",
            "format": "date-time"
          },
          "amount": {
            "type": "integer"
          },
          "accountType": {
            "type": ["null", "string"]
          },
          "accountId": {
            "type": "string"
          },
          "description": {
            "type": ["null", "string"]
          }
        }
      }
    },
    "reconciledBankTransactions": {
      "type": ["null", "array"],
      "items": {
        "type": ["null", "object"],
        "properties": {
          "id": {
            "type": "string"
          },
          "date": {
            "type": "string",
            "format": "date-time"
          },
          "amount": {
            "type": "integer"
          },
          "accountType": {
            "type": ["null", "string"]
          },
          "accountId": {
            "type": "string"
          },
          "description": {
            "type": ["null", "string"]
          },
          "accountingTransactionId": {
            "type": "string"
          },
          "accountingTransactionType": {
            "type": ["null", "string"],
            "enum" : [
              "expense",
              "invoice",
              "payment",
              "payroll",
              "journalEntry",
              "bill",
              "vendorCreditNote",
              "creditNote",
              "billPayment",
              "bankTransaction",
              "transfer",
              "deposit",
              "refund",
              "estimate",
              "inventoryAdjustment",
              "purchaseOrder",
              "taxPayment",
              "taxAdjustment",
              "taxRefund",
              "taxReversal",
              "other",
              "unknown"
            ]
          },
          "isPosting": {
            "type": ["null", "boolean"]
          },
          "isSplitTransaction": {
            "type": ["null", "boolean"]
          },
          "transactionNumber": {
            "type": ["null", "string"]
          },
          "transactionReportId": {
            "type": ["null", "string"]
          },
          "accountRef": {
            "type": ["null", "object"],
            "properties": {
              "id": {
                "type": "string"
              },
              "name": {
                "type": "string"
              },
              "section": {
                "type": ["null", "string"],
                 "enum" : [
                    "Assets",
                    "Expenses",
                    "Income",
                    "Liabilities",
                    "Equity"
                  ]
              },
              "subSection": {
                "type": ["null", "string"],
                "enum" : [
                  "Current Assets",
                  "Non-Current Assets",
                  "Current Liabilities",
                  "Non-Current Liabilities",
                  "Equity",
                  "Income",
                  "Other Income",
                  "Operating Expenses",
                  "Other Expenses",
                  "Cost of Goods Sold"
                ]
              },
              "group": {
                "type": ["null", "string"]
              },
              "subGroup": {
                "type": ["null", "string"]
              }
            }
          },
          "entityRef": {
            "type": ["null", "object"],
            "properties": {
              "id": {
                "type": ["null", "string"]
              },
              "name": {
                "type": ["null", "string"]
              }
            }
          }
        }
      }
    }
  }
}
