{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "reportId": {
      "type": "string"
    },
    "startDate": {
      "type": "string",
      "format": "date-time"
    },
    "endDate": {
      "type": "string",
      "format": "date-time"
    },
    "businessName": {
      "type": "string"
    },
    "serviceName": {
      "type": "string"
    },
    "section": {
      "type": "string",
      "enum" : [
        "Operating Activities",
        "Financing Activities",
        "Investing Activities",
        "Cash"
      ]
    },
    "subSection": {
      "type": "string",
      "enum" : [
        "Payments",
        "Adjustments",
        "Changes",
        "Proceeds",
        "Stock",
        "Intangibles",
        "Property Plant And Equipment",
        "Cash"
      ]
    },
    "group": {
      "type": ["null", "string"]
    },
    "subGroup": {
      "type": ["null", "string"]
    },
    "accountId": {
      "type": ["null", "string"]
    },
    "subAccountId": {
      "type": ["null", "string"]
    },
    "account": {
      "type": "string"
    },
    "subAccount": {
      "type": ["null", "string"]
    },
    "value": {
      "type": "number"
    },
    "fullyQualifiedName": {
      "type": "string"
    },
    "depth": {
      "type": "integer"
    }
  }
}